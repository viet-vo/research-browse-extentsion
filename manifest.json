{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/VoLogo.png",
        "32": "images/VoLogo.png",
        "48": "images/VoLogo.png",
        "128": "images/VoLogo.png"
      },
      "icons": {
        "16": "images/VoLogo.png",
        "32": "images/VoLogo.png",
        "48": "images/VoLogo.png",
        "128": "images/VoLogo.png"
      }
    },
    "manifest_version": 2
  }
